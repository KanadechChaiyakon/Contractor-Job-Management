<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="program.EquipmentListController">
   <children>
      <TableView fx:id="equipmentlist" layoutX="151.0" layoutY="89.0" prefHeight="358.0" prefWidth="498.0">
        <columns>
          <TableColumn prefWidth="75.0" text="Name" />
          <TableColumn prefWidth="75.0" text="Price" />
        </columns>
      </TableView>
      <Button fx:id="back" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#GoBackOnAction" text="Back">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="add" layoutX="729.0" layoutY="14.0" mnemonicParsing="false" onAction="#GoAddEquipmentOnAction" text="Add">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
